config_version=4

_global_script_classes=[ {
"base": "Spatial",
"class": "LogicEntity",
"language": "GDScript",
"path": "res://addons/entity_api/scripts/classes/LogicEntity.gd"
}, {
"base": "Node",
"class": "VMF",
"language": "GDScript",
"path": "res://addons/vmflib/VMF.gd"
} ]
_global_script_class_icons={
"LogicEntity": "res://addons/entity_api/icon.png",
"VMF": ""
}

[application]

config/name="GodotPuzzlemaker"
run/main_scene="res://scenes/Main.tscn"
boot_splash/image="res://images/logo/loading.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.286275, 0.286275, 0.286275, 1 )
config/icon="res://images/logo/icon.png"
config/macos_native_icon="res://images/logo/icon.icns"
config/windows_native_icon="res://images/logo/icon.ico"
config/auto_accept_quit=false
config/quit_on_go_back=false

[autoload]

Globals="*res://scripts/Globals.gd"
PackLoader="*res://scripts/util/PackLoader.gd"
LogicManager="*res://addons/entity_api/scripts/LogicManager.gd"
VMFDataManager="*res://addons/vmflib/VMFDataManager.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/fullscreen=true

[editor_plugins]

enabled=PoolStringArray( "entity_api", "vmflib" )

[gdnative]

singletons=[  ]
singletons_disabled=[  ]

